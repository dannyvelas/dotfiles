{
    "vim.camelCaseMotion.enable": true,
    "vim.sneak": true,
    "vim.sneakReplacesF": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "u"
            ],
            "after": [],
            "commands": [
                {
                    "command": "undo",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "<C-r>"
            ],
            "after": [],
            "commands": [
                {
                    "command": "redo",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "k"
            ],
            "after": [
                "g",
                "k"
            ]
        },
        {
            "before": [
                "j"
            ],
            "after": [
                "g",
                "j"
            ]
        },
        {
            "before": [
                "]",
                "g"
            ],
            "commands": [
                "editor.action.marker.nextInFiles"
            ],
            "silent": true
        },
        {
            "before": [
                "[",
                "g"
            ],
            "commands": [
                "editor.action.marker.prevInFiles"
            ],
            "silent": true
        }
    ],
    "vim.useSystemClipboard": true
}