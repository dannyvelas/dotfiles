{
  "languageserver": {
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": [
        "--lsp"
      ],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml"
      ],
      "filetypes": [
        "haskell",
        "lhaskell"
      ]
    },
    "ccls": {
      "command": "ccls",
      "filetypes": [
        "c",
        "cpp",
        "objc",
        "objcpp"
      ],
      "rootPatterns": [
        ".ccls",
        "compile_commands.json",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "initializationOptions": {
        "cache": {
          "directory": "/tmp/ccls"
        }
      }
    },
    "clojure-lsp": {
      "command": "clojure-lsp",
      "filetypes": ["clojure"],
      "rootPatterns": ["project.clj", "deps.edn"],
      "additionalSchemes": ["jar", "zipfile"],
      "trace.server": "verbose",
      "diagnostic.showUnused": true,
      "diagnostic.showDeprecated": true,
      "diagnostic.highlightPriority": 1000000,
      "initializationOptions": {
        "ignore-classpath-directories": true
      }
    }
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "rust",
    "java",
    "cpp",
    "c",
    "scala",
    "python",
    "go",
    "clojure"
  ],
  "typescript.format.enabled": false,
  "java.home": "/usr/lib/jvm/java-11-openjdk",
  "metals.javaHome": "/usr/lib/jvm/java-11-openjdk",
  "svelte.enable-ts-plugin": true,
  "python.formatting.autopep8Path": "/Users/dannyvelasquez/Library/Python/3.8/bin/autopep8"
}
